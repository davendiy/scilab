n=input("введите длину вектора");
a1=input("a1=");         //введення першого елемента вектора
a=[a1];
for i=2:n
    a1=input("a"+string(i)+"=");     //введення кожного нового елемента вектора
    a=[a,a1];                          //приєднання нового елемента до матриці
end
disp(a)                       //виведення
